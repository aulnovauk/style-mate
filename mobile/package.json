{
  "name": "stylemate-mobile-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "customer": "npm run start --workspace=stylemate",
    "business": "npm run start --workspace=stylemate-business",
    "customer:android": "npm run android --workspace=stylemate",
    "customer:ios": "npm run ios --workspace=stylemate",
    "business:android": "npm run android --workspace=stylemate-business",
    "business:ios": "npm run ios --workspace=stylemate-business"
  }
}
